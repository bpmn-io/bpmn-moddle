{
  "name": "bpmn-moddle",
  "version": "8.1.0",
  "description": "A moddle wrapper for BPMN 2.0",
  "scripts": {
    "all": "run-s generate-schema lint test distro",
    "lint": "eslint .",
    "dev": "npm test -- --watch",
    "test": "mocha --reporter=spec --recursive test/spec test/integration",
    "distro": "run-s build test:build",
    "build": "rollup -c",
    "test:build": "mocha --reporter=spec --recursive test/distro",
    "prepare": "run-s distro",
    "generate-schema": "node tasks/generate-schema.cjs"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/bpmn-moddle"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "umd:main": "dist/bpmn-moddle.umd.cjs",
  "source": "lib/index.js",
  "keywords": [
    "bpmn",
    "moddle",
    "bpmn20",
    "meta-model"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-assertions": "^7.18.6",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "bpmn-in-color-moddle": "^0.1.0",
    "chai": "^4.3.10",
    "cmof-parser": "^0.5.2",
    "eslint": "^8.24.0",
    "eslint-plugin-bpmn-io": "^0.16.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.9.1",
    "xsd-schema-validator": "^0.7.0"
  },
  "dependencies": {
    "min-dash": "^4.0.0",
    "moddle": "^6.2.3",
    "moddle-xml": "^10.1.0"
  },
  "files": [
    "dist",
    "resources",
    "!resources/bpmn/cmof"
  ]
}
